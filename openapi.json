{
  "openapi": "3.0.1",
  "info": {
    "title": "LEAD ONE Plugin",
    "description": "Execute base64 Python code and retrieve results",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://c3p-python-runner.onrender.com"
    }
  ],
  "paths": {
    "/run": {
      "post": {
        "summary": "Execute code",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "code": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Result"
          }
        }
      }
    }
  }
}
